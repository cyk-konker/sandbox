
{
  "id": "/konker-iot/webapp-pipe",
  "cmd": null,
  "cpus": 0.9,
  "mem": 1024,
  "disk": 200,
  "instances": 1,
  "container": {
    "type": "DOCKER",
    "volumes": [],
    "docker": {
      "image": "konkerkobayashi/jenkins-konker",
      "network": "BRIDGE",
      "portMappings": [
        {
          "containerPort": 80,
          "hostPort": 0,
          "protocol": "tcp",
          "name": "http",
          "labels": {}
        }
      ],
      "privileged": true,
      "parameters": [],
      "forcePullImage": false
    }
  },
  "healthChecks": [
    {
      "path": "/",
      "protocol": "HTTP",
      "portIndex": 0,
      "gracePeriodSeconds": 300,
      "intervalSeconds": 60,
      "timeoutSeconds": 20,
      "maxConsecutiveFailures": 3,
      "ignoreHttp1xx": false
    }
  ],
  "labels": {
    "HAPROXY_0_REDIRECT_TO_HTTPS": "false",
    "HAPROXY_GROUP": "konkerbe",
    "HAPROXY_0_BACKEND_STICKY_OPTIONS": "cookie JSESSIONID prefix nocache",
    "HAPROXY_0_BACKEND_REDIRECT_HTTP_TO_HTTPS_WITH_PATH": "redirect scheme https code 301 if !{{ ssl_fc }} host_{cleanedUpHostname} path_{backend}",
    "HAPROXY_0_VHOST": "www.stagingpipe.konkerlabs.net, stagingpipe.konkerlabs.net, api.stagingpipe.konkerlabs.net"
  }
}
